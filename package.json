{
  "name": "cra-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.6",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-scripts": "2.1.8",
    "typescript": "3.3.4000"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:ts": "tsc && tslint --project tsconfig.json",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "format:ts": "prettier --write 'src/**/*.{ts,tsx}' && tslint --fix --project .",
    "format:css": "prettier-stylelint --quiet --write 'src/**/*.scss'",
    "format": "yarn run format:ts && yarn run format:css",
    "format:check": "prettier -c 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0"
  }
}
